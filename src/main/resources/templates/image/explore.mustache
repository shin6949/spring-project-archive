{{>layout/header}}
<link rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css">

<main id="profile">
		<div class="profile__photo-grid">
				{{#posts.content}}
				<!-- 유저가 업로드한 Feed 사진 -->
					<div class="profile__photo">
						<a href="/post/detail/{{id}}">
                            {{#images}}
                                <img src="{{url}}" />
                            {{/images}}
						</a>
					</div>
					{{/posts.content}}
		</div>

    <div class="w3-center">
        {{^posts.isFirst}}
            <a href="/post/explore?page={{prevPage}}"><button id="btn-prev-page" class="w3-btn">&#10094; Prev</button></a>
        {{/posts.isFirst}}
        {{^posts.isLast}}
            <a href="/post/explore?page={{nextPage}}"><button id="btn-next-page" class="w3-btn">Next &#10095;</button></a>
        {{/posts.isLast}}
    </div>
</main>

{{>layout/footer}}
